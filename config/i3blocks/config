[wifi]
command=~/.config/i3blocks/get_wifi.sh
interval=1

[batpourcent]
command=~/.config/i3blocks/get_bat.sh
interval=1

[lockstate]
command=~/.config/i3blocks/get_state.sh
interval=1

# Update time every 5 seconds
[time]
command=date +%T | cut -d ":" -f 1,2
interval=5
